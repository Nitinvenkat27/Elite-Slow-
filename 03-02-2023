public static int birthday(List<Integer> s, int d, int m) {
        int a=0;
        int b=0;
        for(int i=0;i<=s.size()-m;i++){
            for(int j=i;j<i+m;j++){
               a=a+s.get(j); 
            }
            if(a==d){
                b++;
            }
            a=0;
        }
        return b;
    }
